{
    "name": "TPM-rewrite",
    "version": "0.0.1",
    "main": "index.js",
    "bin": "AutoUpdate.js",
    "scripts": {
        "start": "node AutoUpdate.js"
    },
    "pkg": {
        "scripts": [
            "index.js",
            "logger.js",
            "config.js",
            "TPM-bot/*",
            "AutoUpdate.js"
        ],
        "assets": [
            "node_modules/axios/**/*",
            "logs/**/*"
        ],
        "targets": [
            "node18-win-x64",
            "node18-macos-x64",
            "node18-linux-x64"
        ]
    },
    "dependencies": {
        "axios": "^1.7.7",
        "events": "^3.3.0",
        "fs": "^0.0.1-security",
        "golden-fleece": "^1.0.9",
        "json5": "^2.2.3",
        "mineflayer": "^4.20.1",
        "path": "^0.12.7",
        "ws": "^8.18.0"
    }
}
